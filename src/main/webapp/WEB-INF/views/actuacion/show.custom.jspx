	<script type="text/javascript">
		$('#menuTable1').tablePagination({});
		$('#menuTable2').tablePagination({});
		$('#menuTable3').tablePagination({});

		dojo.addOnLoad(function() {
			//Validacion que hace que si estan llenos los valores de enfoque y metodo de la actuacion aparece para cargar los objetivos 				
			//especificos
			var elemento = document.getElementById("_enfoque_id");
			var elemento2 = document.getElementById("_metodo_id");
			var div = document.getElementById("accordionObjetivosEspecificos");
			try {
				if (elemento.innerHTML != '--') {
					if (elemento2.innerHTML != '--') {
						div.style.display = '';
					}
				}
			} catch (e) {
			}

			//Quitar Mensaje de Editar Actuacion con para sellecionar auditor responsable
			var elemento4 = document.getElementById("mensaje_coordinador");
			var div3 = document.getElementById("_responsableAuditor_id");
			try {
				if (div3.innerHTML != "") {
					elemento4.style.display = 'none';
				}
			} catch (e) {
			}
			
			//Validando si hay por lo menos un objetivo especifico y asi te aparece el boton de
			//cargar datos a travez de biblioteca
			var elemento3 = document.getElementById("tituloDefault");
			var div2 = document.getElementById("_boton_biblioteca");
			try {
				elemento3.innerHTML;
			} catch (e) {
				div2.style.display = '';
			}
		});
	</script>
</div>

