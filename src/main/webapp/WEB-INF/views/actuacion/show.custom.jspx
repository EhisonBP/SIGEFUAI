															<spring:url value="/prueba/${actividadActuacion.id}"
																var="show_form_url" />
														</c:if> <c:if test="${actividadActuacion.type eq 'a' }">
															<spring:url
																value="/actividadgeneral/${actividadActuacion.id}"
																var="show_form_url" />
